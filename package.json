{
	"name": "bevy-inspection",
	"displayName": "bevy-inspection",
	"description": "Bevy Inspection",
	"version": "0.1.0",
	"repository": {
		"url": "https://github.com/foxication/bevy-inspection.vcscode"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"categories": [
		"Debuggers",
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "bevy-inspection",
					"title": "Bevy Inspection",
					"icon": "assets/bevy.svg"
				}
			]
		},
		"views": {
			"bevy-inspection": [
				{
					"id": "hierarchyView",
					"name": "Hierarchy"
				},
				{
					"id": "inspectorView",
					"name": "Inspector"
				},
				{
					"id": "resourcesView",
					"name": "Resources",
					"visibility": "collapsed"
				},
				{
					"id": "eventsView",
					"name": "Events",
					"visibility": "collapsed"
				},
				{
					"id": "diagnosticsView",
					"name": "Diagnostics",
					"visibility": "collapsed"
				},
				{
					"id": "schemaView",
					"name": "Schema",
					"visibility": "collapsed"
				}
			]
		},
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.bevyList",
				"title": "Bevy Call List"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^4.0.0",
		"@types/node": "^22.13.4",
		"@types/vscode": "^1.73.0",
		"eslint": "^9.13.0",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0"
	},
	"dependencies": {
		"bevy-remote-protocol": "^1.0.0"
	}
}